@using FitnessHub.Common
@inject Microsoft.Extensions.Configuration.IConfiguration Configuration
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@this.ViewData["Title"] - @GlobalConstants.SystemName</title>

    <!-- Css Styles -->
    <link href="~/lib/fontawesome/css/all.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/checkout.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/chat.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/itemsList.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/site.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/paging.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/flaticon.css" rel="stylesheet" asp-append-version="true">
    <link href="~/css/owl.carousel.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/css/barfiller.css" rel="stylesheet" asp-append-version="true">
    <link href="~/css/magnific-popup.css" rel="stylesheet" asp-append-version="true">
    <link href="~/css/slicknav.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/css/style.css" rel="stylesheet" asp-append-version="true">
    <link href="~/css/myCss.css" rel="stylesheet" asp-append-version="true">

    @*Theme CSS*@

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Muli:300,400,500,600,700,800,900&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Oswald:300,400,500,600,700&display=swap" rel="stylesheet">
</head>

<body>

    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Offcanvas Menu Section Begin -->
    <div class="offcanvas-menu-wrapper">
        <div class="canvas-close">
            <i class="fas fa-times"></i>
        </div>
        <div class="canvas-search search-switch">
            <i class="fa fa-search"></i>
        </div>
        <nav class="canvas-menu mobile-menu">
            <ul>
                <li>
                    <a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li>
                    <a asp-area="" asp-controller="News" asp-action="Index" asp-route-page="1">News</a>
                </li>
                <li>
                    <a asp-area="" asp-controller="TrainingSchedular" asp-action="Index" asp-route-page="1">Training Schedular</a>
                </li>
                <li>
                    <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                </li>
                <li>
                    <a href="" onclick="return false;">Shop</a>
                    <ul class="dropdown">
                        <li><a asp-area="" asp-controller="Suplements" asp-action="Index" asp-route-page="1">Suplements</a></li>
                        <li><a asp-area="" asp-controller="Equipments" asp-action="Index" asp-route-page="1">Equipments</a></li>
                    </ul>
                </li>
                <li>
                    <a asp-area="" asp-controller="Contacts" asp-action="Index">Contacts</a>
                </li>
            </ul>
            <partial name="_LoginPartial" />
        </nav>
        <div id="mobile-menu-wrap"></div>
    </div>
    <!-- Offcanvas Menu Section End -->
    <!-- Header Section Begin -->
    <header class="header-section">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-3">
                    <div class="logo">
                        <a asp-area="" asp-controller="Home" asp-action="Index">
                            <img src="~/images/logo-dark.png" alt="">
                        </a>
                    </div>
                </div>
                <div class="col-lg-6">
                    <nav class="nav-menu">
                        <ul class="middle_navbar">
                            <li>
                                <a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="News" asp-action="Index" asp-route-page="1">News</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="TrainingSchedular" asp-action="Index" asp-route-page="1">Training Schedular</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                            </li>
                            <li>
                                <a href="" onclick="return false;">Shop</a>
                                <ul class="dropdown">
                                    <li><a asp-area="" asp-controller="Suplements" asp-action="Index" asp-route-page="1">Suplements</a></li>
                                    <li><a asp-area="" asp-controller="Equipments" asp-action="Index" asp-route-page="1">Equipments</a></li>
                                </ul>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="Contacts" asp-action="Index">Contacts</a>
                            </li>
                        </ul>
                    </nav>
                </div>
                <div class="col-lg-3">
                    <partial name="_LoginPartial" />
                </div>
            </div>
            <div class="canvas-open">
                <i class="fa fa-bars"></i>
            </div>
        </div>
    </header>
    <!-- Header End -->

    <div>
        <main role="main">
            @this.RenderBody()
        </main>
        <partial name="_CookieConsentPartial" />
    </div>

    <!-- Footer Section Begin -->
    <div class="gettouch-section">
        <div class="container">
            <div class="row">
                <div class="col-md-4">
                    <div class="gt-text">
                        <i class="fa fa-map-marker"></i>
                        <p>bul. Treti Mart 78 <br />Montana, Bulgaria</p>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="gt-text">
                        <i class="fa fa-mobile"></i>
                        <ul>
                            <li>0885840923</li>
                            <li>0882444323</li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="gt-text email">
                        <i class="fa fa-envelope"></i>
                        <p>fitnesshubofficial2020@gmail.com</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <section class="footer-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="copyright-text">
                        <p>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;
                            <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Footer Section End -->

    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js" asp-append-version="true"></script>
    <script src="~/lib/signalr/dist/browser/signalr.min.js" asp-append-version="true"></script>
    <script src="~/js/site.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.magnific-popup.min.js" asp-append-version="true"></script>
    <script src="~/js/masonry.pkgd.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.barfiller.js" asp-append-version="true"></script>
    <script src="~/js/jquery.slicknav.js" asp-append-version="true"></script>
    <script src="~/js/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
    <script src="https://www.google.com/recaptcha/api.js" asp-append-version="true"></script>
    <script src="https://www.google.com/recaptcha/api.js?render=@this.Configuration["GoogleReCaptcha:Key"]"></script>

    @await this.RenderSectionAsync("Scripts", required: false)
</body>
</html>
